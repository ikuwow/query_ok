<%= Rouge::Themes::Base16::Solarized.mode(:dark).render(:scope => '.highlight') %>

.highlight {
  border-radius: 3px;
}

.highlight pre.highlight {
  overflow: scroll;
  padding: 16px;
}

/* inline styles */
p code,
ul code{
  background-color: #f7f7f7;
  border: solid 1px #dddddd;
  border-radius: 1px;
  padding: 0 2px;
}
